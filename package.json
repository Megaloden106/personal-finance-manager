{
  "name": "personal-finance-management-system",
  "version": "1.0.0",
  "description": "Personal Finance MS",
  "main": "index.js",
  "scripts": {
    "install:client": "cd client && npm ci",
    "install:server": "cd sersver && npm ci",
    "postinstall": "npm run install:client && npm run install:server",
    "lint:client": "cd client && eslint --ext .ts,.tsx .",
    "lint:db": "cd database && eslint --ext .ts .",
    "lint:server": "cd server && eslint --ext .ts .",
    "lint": "npm run lint:client && npm run lint:server && npm run lint:db",
    "start": "cd server && npm run start",
    "start:dev": "cd server && npm run start:dev",
    "build": "cd client && $(npm bin)/webpack -p --config environment/webpack.config.js",
    "build:dev": "cd client && $(npm bin)/webpack -d --watch --config environment/webpack.dev.js",
    "prod": "npm run build && npm run start",
    "dev": "npm run build:dev & npm run start:dev",
    "commit": "npx ts-node .circleci/commit-validation.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Megaloden106/personal-finance-manager.git"
  },
  "keywords": [],
  "author": "Eddie Lo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Megaloden106/personal-finance-manager/issues"
  },
  "homepage": "https://github.com/Megaloden106/personal-finance-manager#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.20",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux": "^3.6.31",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "typescript": "^3.5.3"
  }
}
