{
  "name": "personal-finance-management-system",
  "version": "1.0.0",
  "description": "Personal Finance MS",
  "main": "index.js",
  "scripts": {
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "install-all": "npm install && npm run install-client && npm run install-server",
    "lint-client": "eslint --ext .js,.jsx client/",
    "lint-server": "eslint --ext .js,.jsx server/",
    "lint": "npm run lint-client && npm run lint-server",
    "start-prod": "cd server && npm run start",
    "start-dev": "cd server && npm run start-dev",
    "react-prod": "cd client && webpack -p",
    "react-dev": "cd client && webpack -d --watch",
    "prod": "npm run react-prod && npm run start-prod",
    "dev": "npm run react-dev & npm run start-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Megaloden106/personal-finance-manager.git"
